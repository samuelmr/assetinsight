body
  h1 My Asset Insight
  h2 Your personal profile
  p #{locals.content}
  pre #{locals.traits}
  if traits
    div(style="height: 400px; overflow: auto;").well
      for trait in traits
        div.row(class='flag_#{trait.title}')
          div.col-lg-10 
            span #{trait.id}
          div.col-lg-2
            span #{trait.value}
  if viz
    div.row
      div.col-lg-12
        h2 Visualization
        != viz
